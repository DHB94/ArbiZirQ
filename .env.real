# Environment variables for real transaction execution

# RPC URLs
ZIRCUIT_RPC_URL=https://mainnet.zircuit.com
ETHEREUM_RPC_URL=https://ethereum.publicnode.com
POLYGON_RPC_URL=https://polygon.rpc.blxrbdn.com

# API Keys
GUD_API_KEY=your_gud_api_key_here
BITTE_API_KEY=your_bitte_api_key_here

# Server-side wallet private key (REQUIRED for real transactions)
# This should be a wallet with sufficient funds for the trades
EXECUTOR_PRIVATE_KEY=0x... # Replace with your actual private key

# Disable simulation mode
FORCE_SIMULATION=false
NODE_ENV=production